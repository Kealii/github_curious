<% @github_service.following.each do |followed| %>
  <table>
    <thead>
    <caption><h4><%= followed[:login] %>'s Commit Summary</h4></caption>
    <tr>
      <th>Repository</th>
      <th>Branch</th>
    </tr>
    </thead>
    <tbody>
    <% @github_service.commits(followed[:login]).each do |commit| %>
      <tr>
        <td><%= commit[:repo][:name] %></td>
        <td><%= commit[:payload][:ref].split('/').last %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
<% end %>