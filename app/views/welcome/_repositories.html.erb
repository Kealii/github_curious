<table>
  <thead>
  <caption><h4>My Repositories</h4></caption>
  <tr>
    <th>Repository</th>
    <th>URL</th>
  </tr>
  </thead>
  <tbody>
  <% @github_service.repos.each do |repo| %>
    <tr>
      <td><%= repo[:name] %></td>
      <td><%= link_to repo[:html_url], repo[:html_url] %></td>
    </tr>
  <% end %>
  </tbody>
</table>